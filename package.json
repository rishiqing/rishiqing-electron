{
  "name": "rishiqing-electron",
  "version": "3.1.12",
  "author": "北京创仕科锐信息技术有限责任公司",
  "electronVersion": "3.0.10",
  "main": "main.js",
  "env": "beta",
  "debug-url": "http://localhost:99/app",
  "debug-server": "http://localhost:99",
  "MIN_WINDOW_WIDTH": 1218,
  "MIN_WINDOW_HEIGHT": 630,
  "CompanyName": "北京创仕科锐信息技术有限责任公司",
  "FileDescription": "rishiqing",
  "OriginalFilename": "rishiqing.exe",
  "ProductName": "rishiqing",
  "InternalName": "rishiqing.exe",
  "scripts": {
    "start": "electron .",
    "sass": "grunt compassTask",
    "build-mac-beta": "grunt --env=beta && cross-env CHANNEL=beta ARCH=x64 node electron-builder.js",
    "build-mac-release": "grunt --env=release && cross-env CHANNEL=release ARCH=x64 node electron-builder.js",
    "build-ia32-beta": "grunt --env=beta && cross-env CHANNEL=beta ARCH=ia32 node electron-builder.js",
    "build-ia32-release": "grunt --env=release && cross-env CHANNEL=release ARCH=ia32 node electron-builder.js",
    "build-x64-beta": "grunt --env=beta && cross-env CHANNEL=beta ARCH=x64 node electron-builder.js",
    "build-x64-release": "grunt --env=release && cross-env CHANNEL=release ARCH=x64 node electron-builder.js",
    "rdp-mac-beta": "rishiqing-deploy --env=mac-beta",
    "rdp-mac-check": "rishiqing-deploy --env=mac-check",
    "rdp-mac-release": "rishiqing-deploy --env=mac-release",
    "rdp-ia32-beta": "rishiqing-deploy --env=ia32-beta",
    "rdp-ia32-check": "rishiqing-deploy --env=ia32-check",
    "rdp-ia32-release": "rishiqing-deploy --env=ia32-release",
    "rdp-x64-beta": "rishiqing-deploy --env=x64-beta",
    "rdp-x64-check": "rishiqing-deploy --env=x64-check",
    "rdp-x64-release": "rishiqing-deploy --env=x64-release"
  },
  "dependencies": {
    "@sentry/electron": "0.13.0",
    "electron-updater": "4.0.4",
    "handlebars": "4.0.11",
    "is-gzip": "2.0.0",
    "jquery": "2.2.0",
    "lodash": "4.17.4",
    "md5-file": "3.2.3",
    "module-alias": "2.0.3",
    "nedb": "1.8.0",
    "portfinder": "1.0.19",
    "proxy-chain": "0.2.5",
    "request": "2.72.0",
    "vue": "2.5.9",
    "winreg": "1.2.2"
  },
  "devDependencies": {
    "aliyun-sdk": "^1.11.2",
    "cross-env": "^5.1.1",
    "electron-builder": "^20.36.2",
    "electron-winstaller": "^2.3.4",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compass": "^1.1.0",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-uglify-es": "git+https://github.com/gruntjs/grunt-contrib-uglify.git#harmony",
    "grunt-zip": "^0.17.1",
    "rimraf": "^2.6.2",
    "rishiqing-deploy": "^1.0.3",
    "yargs": "^4.1.0"
  }
}
