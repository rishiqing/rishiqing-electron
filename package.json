{
  "name": "rishiqing",
  "version": "2.1.0",
  "main": "main.js",
  "env": "dev",
  "MIN_WINDOW_WIDTH": 1218,
  "MIN_WINDOW_HEIGHT": 620,
  "scripts": {
    "start": "electron main.js",
    "build": "electron-packager . MyApp",
    "buildMac": "electron-packager . rishiqing --platform=darwin --arch=x64 --version=0.37.8  --out=./package/release/ --icon=./res/app.icns --app-bundle-id=release.rishiqing.electron --overwrite --ignore",
    "buildWin32": "electron-packager . rishiqing --platform=win32 --arch=ia32",
    "build-mac-beta": "grunt --env=beta && node ./build.js --arch=x64 --platform=darwin",
    "build-mac-release": "grunt --env=release && node ./build.js --arch=x64 --platform=darwin"
  },
  "dependencies": {
    "jquery": "2.2.0",
    "winreg": "0.0.12"
  },
  "devDependencies": {
    "electron-packager": "7.0.1",
    "electron-prebuilt": "0.37.8",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compass": "^1.1.0",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-uglify": "^0.11.1",
    "yargs": "^4.1.0"
  }
}
